<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="11.1%p"
    android:keyHeight="@dimen/key_height">

    <Row>
        <Key android:codes="5030" android:keyLabel="▼"
            android:keyEdgeFlags="left"   />

        <Key android:codes="119" android:keyLabel="[" android:keyOutputText = "["

            />
        <Key android:codes="101" android:keyLabel="]" android:keyOutputText = "]" />
        <Key android:codes="114" android:keyLabel="(" android:keyOutputText = "(" />
        <Key android:codes="116" android:keyLabel=")" android:keyOutputText = ")" />
        <Key android:codes="121" android:keyLabel="«" android:keyOutputText = "«"/>
        <Key android:codes="117" android:keyLabel="»" android:keyOutputText = "»"/>
        <Key android:codes="111" android:keyLabel="&amp;" android:keyOutputText = "&amp;"/>
        <Key android:codes="-54" android:keyLabel="•"  android:keyEdgeFlags="right"/>
    </Row>

    <Row>

        <Key android:codes="97" android:keyLabel="▲"   android:keyEdgeFlags="left"/>
        <Key android:codes="115" android:keyLabel="{" android:keyOutputText = "}"/>
        <Key android:codes="100" android:keyLabel="}" android:keyOutputText = "}"/>
        <Key android:codes="102" android:keyLabel="#" android:keyOutputText = "#"/>
        <Key android:codes="103" android:keyLabel="cm" android:keyOutputText = "cm"/>
        <Key android:codes="104" android:keyLabel="km" android:keyOutputText = "km"/>
        <Key android:codes="106" android:keyLabel="'" android:keyOutputText = "'"/>
        <Key android:codes="107" android:keyLabel="&quot;" android:keyOutputText = "&quot;"/>
        <Key android:codes="-55" android:keyLabel="1"  android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="-1" android:keyIcon="@drawable/right_triangle"
            android:keyEdgeFlags="left"/>
        <Key android:codes="122" android:keyLabel="m" android:keyOutputText = "m"/>
        <Key android:codes="120" android:keyLabel="g" android:keyOutputText = "g"/>
        <Key android:codes="99" android:keyLabel="x" android:keyOutputText = "x"/>
        <Key android:codes="118" android:keyLabel="y" android:keyOutputText = "y"/>
        <Key android:codes="98" android:keyLabel="z" android:keyOutputText = "z"/>
        <Key android:codes="110" android:keyLabel="•" android:keyOutputText = "•"/>
        <Key android:codes="-5" android:keyIcon="@drawable/ic_keyboard_backspace"
            android:keyWidth="22.2%p" android:keyEdgeFlags="right"
            android:isRepeatable="true"/>
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-3"
            android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyLabel="1" />
        <Key android:codes="32" android:keyLabel="ᐅᖓᓯᓪᓕᑎᖅ"
            android:keyWidth="55.5%p" android:isRepeatable="true"/>
        <Key android:codes="10" android:keyLabel="ᐊᑌ"
            android:keyWidth="22.2%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
