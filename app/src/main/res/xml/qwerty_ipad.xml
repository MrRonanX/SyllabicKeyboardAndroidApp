<Keyboard
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="11.1%p"
    android:keyHeight="@dimen/key_height_ipad">

    <Row>
        <Key android:codes="113" android:keyLabel="▼" android:keyEdgeFlags="left"    />

        <Key android:codes="119" android:keyLabel="ᐁ" android:iconPreview="@drawable/down_arrow" android:keyOutputText = "ᐁ"
            android:popupCharacters="ᐁᐯ" android:popupKeyboard="@layout/key_preview" />
        <Key android:codes="101" android:keyLabel="ᐯ" android:keyOutputText = "ᐯ"
            android:popupCharacters="ᐯᐯ" android:popupKeyboard="@layout/key_preview" />
        <Key android:codes="114" android:keyLabel="ᑌ" android:keyOutputText = "ᑌ" />
        <Key android:codes="116" android:keyLabel="ᑫ" android:keyOutputText = "ᑫ" />
        <Key android:codes="121" android:keyLabel="ᒉ" android:keyOutputText = "ᒉ"/>
        <Key android:codes="117" android:keyLabel="ᒣ" android:keyOutputText = "ᒣ"/>
        <Key android:codes="111" android:keyLabel="ᓀ" android:keyOutputText = "ᓀ"/>
        <Key android:codes="-10" android:keyLabel="•"  android:keyEdgeFlags="right"/>
    </Row>

    <Row>

        <Key android:codes="97"  android:keyLabel="▲"   android:keyEdgeFlags="left"/>
        <Key android:codes="115" android:keyLabel="ᓭ"  android:popupCharacters="ᐁᐄ"
            android:popupKeyboard="@layout/key_preview"
            android:keyOutputText = "ᓭ"/>
        <Key android:codes="100" android:keyLabel="ᓓ" android:keyOutputText = "ᓓ"/>
        <Key android:codes="102" android:keyLabel="ᔦ" android:keyOutputText = "ᔦ"/>
        <Key android:codes="103" android:keyLabel="ᕓ" android:keyOutputText = "ᕓ"/>
        <Key android:codes="104" android:keyLabel="ᕂ" android:keyOutputText = "ᕂ"/>
        <Key android:codes="106" android:keyLabel="ᙯ" android:keyOutputText = "ᙯ"/>
        <Key android:codes="107" android:keyLabel="ᙰ" android:keyOutputText = "ᙰ"/>
        <Key android:codes="-11"  android:keyLabel="••"  android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="-1"  android:keyIcon="@drawable/right_triangle" android:keyEdgeFlags="left"/>
        <Key android:codes="122" android:keyLabel="."  android:popupCharacters="ᐁᐄ" android:popupKeyboard="@layout/key_preview"
            android:keyOutputText = "."/>
        <Key android:codes="120" android:keyLabel="," android:keyOutputText = ","/>
        <Key android:codes="99" android:keyLabel="\?" android:keyOutputText = "\?"/>
        <Key android:codes="118" android:keyLabel="!" android:keyOutputText = "!"/>
        <Key android:codes="98" android:keyLabel=";" android:keyOutputText = ";"/>
        <Key android:codes="110" android:keyLabel=":" android:keyOutputText = ":"/>
        <Key android:codes="-5" android:keyIcon="@drawable/ic_keyboard_backspace"
            android:keyWidth="22.2%p"  android:keyEdgeFlags="right"
            android:isRepeatable="true"/>
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-3" android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyLabel="1" />
        <Key android:codes="32" android:keyLabel="ᐅᖓᓯᓪᓕᑎᖅ"
            android:keyWidth="55.5%p" android:isRepeatable="true"/>
        <Key android:codes="10" android:keyLabel="ᐊᑌ"
            android:keyWidth="22.2%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
